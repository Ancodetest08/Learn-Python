>>> i = 1
>>> while i <= 5:
		print(i)
 		i += 1

>>> for i in range(1, 5):
		print(i)
1
2
3
4

>>> name = 'Codelearn'
>>> for c in name:
	print(c)
	
>>> for i in range(1, 11):
	if i == 6:
		break
	print(i)
	
>>> for i in range(1, 20):
	if i % 2 == 0:
		continue
	print(i)

>>> print(round(2.665, 2))
1.67
