list = ['a', 'e', 'i', 'u']
list[0]							# 'a'
len(list)						# 4
list.insert(3, 'o')				# ['a', 'e', 'i', 'o', 'u']
list.remove('a')				# ['e', 'i', 'o', 'u']
list.pop(1)						# ['e', 'o', 'u']
list.append(4)					# ['e', 'o', 'u', 4]

list = [5, 8, 9, 6, 7, 8]
max(list)						# 9
min(lst)						# 5
list.reverse()					# [8, 7, 6, 9, 8, 5]
list.sort()						# [5, 6, 7, 8, 8, 9]
list.sort(reverse = True)		# [9, 8, 8, 7, 6, 5]
list.count(8)					# 2
list.clear()					# []
