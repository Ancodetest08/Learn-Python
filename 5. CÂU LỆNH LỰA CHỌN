>>> n = int(input())
>>> if n % 2 == 0:
		print('n is an even number')
	else:
 		print('n is an odd number')

>>>	a, b = 10, 20
>>> print('Both a and b are equal' if a == b else 'a is greater than b' if a > b else 'b is greater than a')

>>> a = int(input())
>>> b = int(input())
>>> if a == b:
		print('a = b')
>>> elif a > b:
 		print('a > b')
	else:
		print('a < b')
