print(5 // 2)							# 2
print(2 ** 3)							# 8

print('Code' in 'Codelearn')			# True
print('Py' not in 'Python')				# False

a = 5
b = 7
print(a is b)							# False
print(a is not b)						# True

round(2.665, 2)							# 2.67
x = 12.3456789
print('The value of x is %3.2f' %x)    	# The value of x is 12.35
